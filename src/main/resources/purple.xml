<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">
<RSyntaxTheme version="1.0">
    <!--  General editor colors. -->
    <background color="1E001E"/>
    <caret color="FFFFFF"/>
    <selection useFG="false" bg="4A004A" roundedEdges="false"/>
    <currentLineHighlight color="2A002A" fade="false"/>
    <marginLine fg="8A2BE2"/>
    <markAllHighlight color="4A004A"/>
    <markOccurrencesHighlight color="4A004A" border="false"/>
    <matchedBracket fg="FFD700" bg="1E001E" highlightBoth="true" animate="true"/>
    <hyperlinks fg="DA70D6"/>
    <secondaryLanguages>
        <language index="1" bg="2A052A"/>
        <language index="2" bg="2A0A2A"/>
        <language index="3" bg="2A002A"/>
    </secondaryLanguages>
    <!-- Gutter styling. -->
    <gutterBorder color="2A002A"/>
    <lineNumbers fg="8A2BE2"/>
    <foldIndicator fg="9370DB" armedFg="DA70D6" iconBg="1E001E" iconArmedBg="2A002A"/>
    <iconRowHeader activeLineRange="8A2BE2"/>
    <!-- Syntax tokens. -->
    <tokenStyles>
        <style token="IDENTIFIER" fg="FFFFFF"/>
        <style token="RESERVED_WORD" fg="9370DB" bold="true"/>
        <style token="RESERVED_WORD_2" fg="DA70D6" bold="true"/>
        <style token="ANNOTATION" fg="FFD700"/>
        <style token="COMMENT_DOCUMENTATION" fg="B19CD9"/>
        <style token="COMMENT_EOL" fg="B19CD9"/>
        <style token="COMMENT_MULTILINE" fg="B19CD9"/>
        <style token="COMMENT_KEYWORD" fg="FFD700"/>
        <style token="COMMENT_MARKUP" fg="FFD700"/>
        <style token="FUNCTION" fg="DA70D6"/>
        <style token="DATA_TYPE" fg="9370DB" bold="true"/>
        <style token="LITERAL_BOOLEAN" fg="FFD700" bold="true"/>
        <style token="LITERAL_NUMBER_DECIMAL_INT" fg="FFD700"/>
        <style token="LITERAL_NUMBER_FLOAT" fg="FFD700"/>
        <style token="LITERAL_NUMBER_HEXADECIMAL" fg="FFD700"/>
        <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="FF69B4"/>
        <style token="LITERAL_CHAR" fg="FF69B4"/>
        <style token="LITERAL_BACKQUOTE" fg="FF1493"/>
        <style token="MARKUP_TAG_DELIMITER" fg="FF1493"/>
        <style token="MARKUP_TAG_NAME" fg="DA70D6" bold="true"/>
        <style token="MARKUP_TAG_ATTRIBUTE" fg="DDA0DD"/>
        <style token="MARKUP_TAG_ATTRIBUTE_VALUE" fg="FF69B4"/>
        <style token="MARKUP_COMMENT" fg="B19CD9"/>
        <style token="MARKUP_DTD" fg="FFD700"/>
        <style token="MARKUP_PROCESSING_INSTRUCTION" fg="FFD700"/>
        <style token="MARKUP_CDATA" fg="FFFFFF"/>
        <style token="MARKUP_CDATA_DELIMITER" fg="9370DB"/>
        <style token="MARKUP_ENTITY_REFERENCE" fg="BA55D3"/>
        <style token="OPERATOR" fg="9370DB"/>
        <style token="PREPROCESSOR" fg="BA55D3"/>
        <style token="REGEX" fg="FF1493"/>
        <style token="SEPARATOR" fg="FFFFFF"/>
        <style token="VARIABLE" fg="BA55D3" bold="false"/>
        <style token="WHITESPACE" fg="FFFFFF"/>
        <style token="ERROR_IDENTIFIER" fg="FFFFFF" bg="8B008B"/>
        <style token="ERROR_NUMBER_FORMAT" fg="FFFFFF" bg="8B008B"/>
        <style token="ERROR_STRING_DOUBLE" fg="FFFFFF" bg="8B008B"/>
        <style token="ERROR_CHAR" fg="FFFFFF" bg="8B008B"/>
    </tokenStyles>
</RSyntaxTheme>